             <property name="styleSheet">
              <string notr="true">/* QCheckBox 基础样式 */
QRadioButton {
    border: 1px solid;
    font: bold 15px &quot;Microsoft YaHei UI&quot;;
	border-radius:3px;
    color: #A0A0A0;
    background-color: transparent;
    padding: 4px;
    margin: 2px;
}

/* 复选框指示器（未选中状态） */
QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 2px solid #50AFFF;       /* 未选中时边框为蓝色 */
    background-color: #50AFFF;       /* 未选中时背景为蓝色 */
    border-radius: 2px;              /* 轻微圆角 */
}

/* 选中状态 */
QRadioButton::indicator:checked {
    border: 2px solid #FFFFFF;       /* 选中时边框为白色 */
    background-color: #50AFFF;       /* 选中时背景为蓝色 */
}

/* 悬停状态（未选中时） */
QRadioButton::indicator:hover {
    border: 2px solid #FFFFFF;       /* 选中时边框为白色 */
    background-color: #50AFFF;       /* 选中时背景为蓝色 */
}



/* 禁用状态 */
QRadioButton:disabled {
    color: #606060;
}

QRadioButton:disabled::indicator {
    border-color: #444;              /* 禁用时边框为深灰 */
    background-color: #202020;       /* 禁用时背景为深灰 */
}

QRadioButton:disabled::indicator:checked {
    border-color: #606060;           /* 禁用选中时边框为中灰 */
    background-color: #303030;
}

/* 文字悬停状态 */
QRadioButton:hover {
    color: #B0B0B0;
}

QRadioButton:checked {
    color: #FFFFFF;                  /* 选中时文字为白色 */
}</string>
